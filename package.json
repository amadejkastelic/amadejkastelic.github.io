		"flowbite": "^4.0.1",
		"flowbite-svelte": "^1.31.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.1",
		"@fontsource/noto-sans": "^5.2.10",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"bun2nix": "^2.0.6",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
